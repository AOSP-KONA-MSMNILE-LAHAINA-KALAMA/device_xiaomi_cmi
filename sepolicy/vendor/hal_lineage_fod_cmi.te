allow hal_lineage_fod_cmi vendor_sysfs_graphics:dir search;
allow hal_lineage_fod_cmi vendor_sysfs_graphics:file rw_file_perms;

allow hal_lineage_fod_cmi sysfs_fod:file rw_file_perms;
